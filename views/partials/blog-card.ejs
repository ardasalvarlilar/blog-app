<div class="card mb-3">
  <div class="row">
    <div class="col-md-4">
      <img src="/static/images/<%= image %>" alt="" class="img-fluid">
    </div>
    <div class="col-md-8">
      <div class="card-body">
        <h5 class="card-title">
          <a href="/blogs/<%= id %>"><%= title %></a>
        </h5>
        <p class="card-text"><%= subtitle %></p>
      </div>
    </div>
  </div>
</div>